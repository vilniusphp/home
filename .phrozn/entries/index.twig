{% for key, event in this.events %}
<div class="event">
    <h3>{{ event.name }}</h3>
    <b>{{ event.date }}</b>

    <br>
    {% if key == 0 %}
        {% if event.place %}
            {{ event.place.name }}, {{ event.place.address }}
        {% else %}
            Vieta tikslinama.
        {% endif %}
    {% endif %}

    {% if event.github %}
    <a class="label label-info" href="{{ event.github }}">Medžiaga @ GitHub</a>
    {% endif %}

    <h4>Pranešėjai</h4>
    <ol>
        {% if event.speakers|length == 0 %}
        Turi ką papasakoti? Rašyk mums!
        {% endif %}

        {% for speaker in event.speakers %}
        <li>
            <strong>{{ speaker.name }}</strong>
            {% if speaker.linkedin %}
            <a href="{{ speaker.linkedin }}" title="{{ speaker.name }}" target="_blank"><img src="media/img/linkedin.png" alt="" /></a>
            {% endif %}
            <br>
            {{ speaker.title }}
        </li>
        {% endfor %}
    </ol>

    {% if event.afterparty and key == 0 %}
    <h4>After party</h4>
    <p>
        {{ event.afterparty.name}} {{ event.afterparty.address }}
        (<a href="{{ event.afterparty.mapurl }}" target="_blank">žemėlapis</a>).
    </p>
    {% endif %}
</div>
<hr>
{% endfor %}
